
<div id="info-search">
    <form action="searchRentHouse" class="form-search-rent-house" method="post">
        <div id="info-search-box">
            <div class="grouper">
                <div class="sel-rent-house">
                    <p class="house-info-form-title">Từ Khóa</p>
                    <%if(keyword ==''){%>
                      <input class="sel-keyword" placeholder="Nhà trọ" name="keyword" value="" />
                    <%}else{%>
                      <input class="sel-keyword" placeholder="Nhà trọ" name="keyword" value="<%=keyword%>" />
                    <%}%>
                </div>
                <div class="sel-rent-house">
                    <p class="house-info-form-title">Ngày Đăng</p>
                    <select class="sel-time-post" name="time_post">
                        <option value="">Ngày đăng</option>
                        <option value="1">1 ngày trước</option>
                        <option value="2">2 ngày trước</option>
                        <option value="3">3 ngày trước</option>
                        <option value="4">1 tuần trước </option>
                        <option value="5">1 tháng trước</option>
                    </select>
                </div>

                <div class="sel-rent-house">
                    <p class="house-info-form-title">Mức Giá</p>
                    <div class="sel-price">
                        <input id="min-price" class="input-price" placeholder="Min" name="minPrice" value="" />
                        <input id="max-price" class="input-price" placeholder="Max" name="maxPrice" value="" />
                    </div>
                </div>

                <div class="sel-rent-house">
                    <p class="house-info-form-title">Diện Tích</p>
                    <select class="sel-area" name="area">
                        <option value="">Diện tích</option>
                        <option value="1">Dưới 30 m2</option>
                        <option value="2">30 - 50 m2</option>
                        <option value="3">50 - 80 m2 </option>
                        <option value="4">80 - 100 m2</option>
                        <option value="5">150 - 200 m2</option>
                    </select>
                </div>

            </div>
        </div>

        <div id="search-buttons">
            <div id="button-2">
                <input type="submit" name="base-search-button" id="search_rh_button" value=" Tìm Kiếm "  class="ok-button" />
            </div>
        </div>
    </form>
</div>
<div class="show_frm_search"></div>
<div id="total_search"></div>
<div class="clear"></div>
<div id="result-search"></div>